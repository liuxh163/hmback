{
  "name": "hmback",
  "version": "0.0.1",
  "description": "Hm101 project",
  "author": "Liuxh",
  "private": true,
  "scripts": {
    "watch": "NODE_ENV=development nodemon app.js --exec",
    "test": "NODE_ENV=testing jest --forceExit",
    "build": "NODE_ENV=production babel src -d build && npm run pretty",
    "pretty": "prettier --write --print-width 80 --single-quote --trailing-comma es5 --tab-width 4 --no-semi 'src/**/*.js' 'tests/**/*.js'",
    "start-production": "NODE_ENV=production pm2 start ecosystem.json",
    "clean": "npm cache clean --force"
  },
  "license": "MIT",
  "homepage": "https://github.com/liuxh163/hmback#readme",
  "dependencies": {
    "@alicloud/sms-sdk": "^1.1.6",
    "@sendgrid/mail": "6.2.1",
    "ali-oss": "^6.0.2",
    "axios": "^0.18.0",
    "bcrypt": "^2.0.1",
    "date-fns": "^1.29.0",
    "dotenv": "^6.0.0",
    "faker": "^4.1.0",
    "fs-extra": "^6.0.1",
    "ioredis": "^3.2.2",
    "jest": "^23.1.0",
    "knex": "^0.14.6",
    "koa": "^2.5.1",
    "koa-body": "^4.0.6",
    "koa-bodyparser": "^4.2.1",
    "koa-json-error": "^3.1.2",
    "koa-ratelimit": "^4.1.1",
    "koa-router": "^7.4.0",
    "koa-useragent": "^1.1.0",
    "koa2-cors": "^2.0.6",
    "log4js": "^2.8.0",
    "mysql": "^2.15.0",
    "node-pinyin": "^0.2.3",
    "npm": "^6.1.0",
    "pm2": "^2.10.4",
    "randexp": "^0.4.9",
    "xmlreader": "^0.2.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "nodemon": "^1.17.5",
    "prettier": "^1.13.4"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "safari >= 7"
            ]
          }
        }
      ]
    ]
  },
  "keywords": [
    "koa",
    "koa2",
    "api",
    "boilerplate",
    "vue",
    "spa",
    "backend",
    "jwt",
    "pm2",
    "mysql",
    "database"
  ],
  "jest": {
    "testEnvironment": "node"
  }
}
